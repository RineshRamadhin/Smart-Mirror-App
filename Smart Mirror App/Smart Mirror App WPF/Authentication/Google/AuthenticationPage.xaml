<Page x:Class="Smart_Mirror_App_WPF.Authentication.Google.AuthenticationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Smart_Mirror_App_WPF.Authentication.Google"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="GoogleAuthentication">

    <Grid>
        <StackPanel>
            <TextBlock></TextBlock>
            <Button Content="Test"></Button>
         
            <Button Content="Login" Command="{Binding Path=LoginGoogle}"></Button>
        </StackPanel>
    </Grid>
</Page>
